# Airflow Configuration
AIRFLOW_PROJ_DIR=./src
AIRFLOW__CORE__LOAD_EXAMPLES=false
AIRFLOW_UID=1000

# Wasabi S3 Storage Configuration
# Get your credentials from Wasabi console: https://console.wasabisys.com
WASABI_ENDPOINT=s3.wasabisys.com
WASABI_ACCESS_KEY=your-wasabi-access-key-here
WASABI_SECRET_KEY=your-wasabi-secret-key-here
WASABI_BUCKET_NAME=your-wasabi-bucket-name

# url for retrieve proxies (from webshare.io, returned in txt format, fill free to update proxy manager if need on srs/scripts/utils/proxy.py)
PROXY_URL=your_webshare_url

# Minimum Points API Configuration
# External API endpoint for sending minimum points data
MIN_POINTS_API_URL=https://your-api.com/api/min-points
MIN_POINTS_API_KEY=your-api-key-here

# Superset Configuration
# Secret key for encryption - MUST be the same across all machines
SUPERSET_SECRET_KEY=YourSuperSecretKeyForSuperset_ChangeThisInProduction_12345

# External PostgreSQL for Superset (e.g., AWS RDS)
# If using local postgres, set SUPERSET_DB_HOST=postgres
SUPERSET_DB_HOST=your-rds-endpoint.amazonaws.com
SUPERSET_DB_PORT=5432
SUPERSET_DB_USER=airflow
SUPERSET_DB_PASSWORD=airflow
SUPERSET_DB_NAME=superset
